{
    "rewrites": [
        {
            "source": "/products/:id",
            "destination": "/api/og?url=/products/:id"
        },
        {
            "source": "/",
            "destination": "/api/og?url=/"
        },
        {
            "source": "/((?!api|_next|favicon.ico|images|og).*)",
            "destination": "/index.html"
        }
    ]
}